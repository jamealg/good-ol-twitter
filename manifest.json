{
  "name": "Good ol Twitter",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "See only your followers' tweets tweets. No likes. No retweets.",
  "homepage_url": "https://github.com/jamealg/good-ol-twitter",
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "js": [
        "src/inject.js"
      ]
    }
  ]
}
